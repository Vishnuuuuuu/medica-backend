{
  "name": "healthcare-shift-logging-backend",
  "version": "1.0.0",
  "description": "Healthcare shift logging backend with GraphQL and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && npx prisma generate",
    "render-build": "npm install && npm run build && npx prisma db push",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:setup": "npx prisma db push && npx prisma generate && npm run db:seed",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:verify": "ts-node scripts/verify-db.ts",
    "generate": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "health": "ts-node src/health-check.ts",
    "setup": "npm install && npx prisma generate",
    "clean": "rm -rf dist node_modules"
  },
  "keywords": [
    "healthcare",
    "shift-logging",
    "graphql",
    "prisma",
    "auth0"
  ],
  "author": "",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth-server": "^2.0.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.0.5",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2"
  }
}
